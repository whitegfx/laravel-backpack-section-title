{
    "name": "stats4sd/laravel-backpack-section-title",
    "description": "A custom Backpack Field that lets you easily add custom headings and information boxes to split up the form.",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Mills",
            "email": "d.e.mills@stats4sd.org",
            "homepage": "https://stats4sd.org"
        }
    ],
    "homepage": "https://github.com/stats4sd/laravel-backpack-section-title",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "LaravelBackpackSectionTitle"
    ],
    "require": {
        "illuminate/support": "~10|~11",
        "backpack/crud": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.5|~11.0",
        "orchestra/testbench": "~10|~11"
    },
    "autoload": {
        "psr-4": {
            "Stats4sd\\LaravelBackpackSectionTitle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stats4sd\\LaravelBackpackSectionTitle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stats4sd\\LaravelBackpackSectionTitle\\AddonServiceProvider"
            ]
        }
    }
}
